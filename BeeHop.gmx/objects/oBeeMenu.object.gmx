    // Make sure image_xscale isn't 0
    if (image_xscale == 0){
        image_xscale = sign(xDir)*0.001;
    }
    
    var _ease = EaseInOutQuad;
    
    // Ease x flip -- IF flipping back to same relative side ELSE flipping to completely other side
    if (sign(xDir) == sign(image_xscale))
    {
        var _duration = flipSpeed-(flipSpeed*abs(image_xscale));
        TweenFire(id, _ease, TWEEN_MODE_ONCE, true, 0.0, _duration, image_xscale__, image_xscale, 1*sign(image_xscale)); 
    }
    else
    {
        var _duration = flipSpeed*0.5 + 0.5*flipSpeed*(abs(image_xscale));
        TweenFire(id, _ease, TWEEN_MODE_ONCE, true, 0.0, _duration, image_xscale__, image_xscale, -1*sign(image_xscale)); 
    }
}

//Scale
