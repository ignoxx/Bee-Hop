            <kind>1</kind>
            <string>///Wait for correct orientation before continuing

var try_to_continue = YES;

if (global.DEVICE_IS_TOUCH){
    if (instance_exists(ctr_screen)){
        if (ctr_screen.actual_rotation != ctr_screen.desired_rotation){
            try_to_continue = NO;
        }
    }
}

// NOT A BROWSER
if (try_to_continue){
    // If splash screen enabled
    if (global.SPLASH_SCREEN){
        if (global.SPLASH_OVER){
            room_goto_next();
        }
        else
        // If ctr_splash does not exist
        if !(instance_exists(ctr_splash)){
            // Create the splash screen
            instance_create(0, 0, ctr_splash);
        }
    }
    else
    {
        room_goto_next();
    }
}
