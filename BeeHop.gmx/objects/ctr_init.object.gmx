            <string>///2     Domain Locking

/*
    Domain locking prevents websites from hosting your game if they have not purchased a license.

    IMPORTANT: Make sure you ask publishers for the entire list of websites that they plan on hosting your game. 
    You will need to add all these websites to the list below, otherwise your publisher will not be happy!

    NOTE: The 'Invalid Domain' message is drawn in ctr_screen Draw GUI event.
*/

//Toggle Domain Locking On/Off
global.DOMAIN_LOCK_ENABLE = NO;

if (global.DOMAIN_LOCK_ENABLE == YES)
&amp;&amp; (os_browser != browser_not_a_browser){
    global.VALID_DOMAIN = NO;
    var valid_sites = ds_list_create(); //Create a list of valid domains
    
    ds_list_add(valid_sites,"127.0.0"); //127.0.0, Keep this for testing
    ds_list_add(valid_sites,"10.1.1"); //10.1.1,   Keep this for testing
    ds_list_add(valid_sites,"dropbox"); //Dropbox, keep this for testing   
        
    //Websites authorized to host this game
    ds_list_add(valid_sites,"sponsorwebsite.com");
    ds_list_add(valid_sites,"f528764d624db129b32c21fbca0cb8d6");
    ds_list_add(valid_sites,"f528764d624db129b32c21fbca0cb8d6");
    
    //Get the URL the game is hosted on
    var thisURL = url_get_domain();
    var thisURLhash = md5_string_utf8(url_get_domain());

    /* UNCOMMENT THIS TO FIND HASH
    Use the below codes to find out the URL hash (uncomment and edit it)
    
    //Get the URL hash for a given domain. You must use the EXACT domain the sponsor will be using
    jchtml5_prompt("Copy the below hash and add it to the ds_list in your game","sponsorwebsite");
    
    //Get URL hash for the current domain
    jchtml5_alert("The URL is " + url_get_domain());
    jchtml5_prompt("Copy the below hash and add it to the ds_list in your game",thisURLhash);
    */
    
    // Find if the url is valid
    for (var i=0;i&lt;ds_list_size(valid_sites);i++){
        if (string_count(ds_list_find_value(valid_sites,i),thisURL) &gt; 0)
        or (ds_list_find_value(valid_sites,i) == thisURLhash){
            global.VALID_DOMAIN = YES;
            i=ds_list_size(valid_sites)+1;
        }
    }
    
    //Destroy the list
    ds_list_destroy(valid_sites);
}
