// !!! Do not modify or move !!!

//Only create screen controller if the game is in a browser (mobile or desktop)
if (global.IS_BROWSER)instance_create(-100,0,ctr_screen);


/********************************************
               Start the game
********************************************/

global.MESSAGE_INVALID = "";

//The domain is not valid
if !(global.VALID_DOMAIN){
    global.MESSAGE_INVALID = INVALID_DOMAIN;
}

//If there was a problem
if (global.MESSAGE_INVALID != ""){
    instance_destroy();
    exit;
}

